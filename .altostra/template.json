{
	"name": "Static Website",
	"description": "A static website with CDN distribution",
	"includes": [
		"/public/**",
		"/package*.json",
		"/.altoignore",
		"/.gitignore",
		"/README.md",
		"/LICENSE"
	],
	"excludes": [],
	"blueprints": [
		{
			"id": "StaticWebSite",
			"name": "Static Website",
			"description": "Static Website Template",
			"version": "2019-10-24",
			"connections": {
				"SiteCdn01": {
					"SiteFiles01": {
						"bucketRoot": "",
						"from": "SiteCdn01",
						"route": "",
						"to": "SiteFiles01",
						"type": "connection.dependency.cdn-storage"
					}
				},
				"Domain01": {
					"SiteCdn01": {
						"type": "connection.domain",
						"from": "Domain01",
						"to": "SiteCdn01",
						"hostname": "hostname",
						"certificate": {
							"type": "cert.stored",
							"uri": "arn:aws:acm:us-east-1:123456789012:certificate/00000000-0000-0000-0000-000000000000"
						}
					}
				}
			},
			"metadata": {
				"designer": {
					"resourcePositions": {
						"SiteCdn01": {
							"x": 409,
							"y": 282,
							"z": 0
						},
						"SiteFiles01": {
							"x": 648,
							"y": 282,
							"z": 0
						},
						"Domain01": {
							"x": 172,
							"y": 225,
							"z": 0
						}
					}
				}
			},
			"parameters": {},
			"resources": {
				"SiteCdn01": {
					"defaultRoot": "index.html",
					"id": "SiteCdn01",
					"isSPA": false,
					"name": "site-cdn",
					"tls": {
						"httpRedirection": true,
						"useTLS": true
					},
					"type": "resource.cdn"
				},
				"SiteFiles01": {
					"accessLevel": "public-read",
					"filesSources": [
						{
							"path": "public",
							"type": "files-source.project-dir"
						}
					],
					"id": "SiteFiles01",
					"name": "site-files",
					"type": "resource.file-store"
				},
				"Domain01": {
					"type": "resource.domain",
					"name": "domain",
					"domainName": "example.com",
					"dnsRegistration": false,
					"id": "Domain01"
				}
			},
			"unsafe": false
		}
	]
}