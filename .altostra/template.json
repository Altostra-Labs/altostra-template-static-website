{
	"name": "Static Website",
	"description": "A static website with CDN distribution",
	"includes": [
		"/public/**"
	],
	"excludes": [],
	"blueprints": [
		{
			"id": "StaticWebSite",
			"name": "Static Website",
			"description": "Static Website Template",
			"version": "2019-10-24",
			"connections": {
				"SiteCdn01": {
					"SiteFiles01": {
						"from": "SiteCdn01",
						"to": "SiteFiles01",
						"bucketRoot": "",
						"route": "",
						"type": "connection.dependency.cdn-storage"
					}
				}
			},
			"metadata": {
				"designer": {
					"resourcePositions": {
						"SiteFiles01": {
							"x": 800,
							"y": 350,
							"z": 0
						},
						"SiteCdn01": {
							"x": 300,
							"y": 350,
							"z": 0
						}
					}
				}
			},
			"resources": {
				"SiteFiles01": {
					"accessLevel": "public-read",
					"filesSources": [
						{
							"path": "public",
							"type": "files-source.project-dir"
						}
					],
					"name": "site-files",
					"type": "resource.file-store",
					"id": "SiteFiles01"
				},
				"SiteCdn01": {
					"defaultRoot": "index.html",
					"isSPA": false,
					"name": "site-cdn",
					"type": "resource.cdn",
					"id": "SiteCdn01"
				}
			}
		}
	]
}